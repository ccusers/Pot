<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.pro.domain">

	<class name="Bill" table="Bill" lazy="true">
		
		<id name="bill_id">
			<generator class="native"/>
		</id>
		
		<property name="money_ying"/>
		
		<property name="money_true"/>
		
		<property name="time_acc"/>
		
		<property name="state_acc"/>
		
		<property name="if_invo"/>
		
		<property name="text_acc"/>
					
	</class>
	
</hibernate-mapping>