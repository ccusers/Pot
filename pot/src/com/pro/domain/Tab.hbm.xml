<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.pro.domain">

	<class name="Tab" table="AuctionUser" lazy="Tab">
		<comment>Users may bid for or sell auction items.</comment>
		
		<id name="tab_id">
			<generator class="native"/>
		</id>
		<property name="tab_name"></property>
		<many-to-one name="TabState" column="stateId"></many-to-one>
		<property name="tab_type"></property>
		<set name="OrderTableSet" table="tabortab">
			<key column="tab_id"></key>
			<many-to-many class="OrderTable" column="order_id"></many-to-many>
		</set>
		<property name="tab_comeDate"></property>
		<property name="tab_con"></property>
		<property name="tab_other"></property>
		
		
	</class>
	
</hibernate-mapping>